        "\n",
        "class AnimatedBackground(Widget):\n",
        "    def __init__(self, **kwargs):\n",
        "        super().__init__(**kwargs)\n",
        "        self.particles = []\n",
        "        self.num_particles = 25\n",
        "        self.init_particles()\n",
        "        Clock.schedule_interval(self.update, 0.033)\n",
        "    def init_particles(self):\n",
        "        for i in range(self.num_particles):\n",
        "            self.particles.append({'x': random.random(), 'y': random.random(), 'size': random.uniform(1, 3), 'speed_x': random.uniform(-0.001, 0.001), 'speed_y': random.uniform(-0.001, 0.001), 'opacity': random.uniform(0.1, 0.5)})\n",
        "    def update(self, dt):\n",
        "        self.canvas.clear()\n",
        "        with self.canvas:\n",
        "            Color(0.04, 0.04, 0.04, 1)\n",
        "            Rectangle(pos=self.pos, size=self.size)\n",
        "            Color(0, 0.25, 0, 0.08)\n",
        "            grid_size = dp(50)\n",
        "            for x in range(0, int(self.width), int(grid_size)):\n",
        "                Line(points=[x, 0, x, self.height], width=0.5)\n",
        "            for y in range(0, int(self.height), int(grid_size)):\n",
        "                Line(points=[0, y, self.width, y], width=0.5)\n",
        "            for p in self.particles:\n",
        "                p['x'] += p['speed_x']\n",
        "                p['y'] += p['speed_y']\n",
        "                if p['x'] < 0 or p['x'] > 1: p['speed_x'] *= -1\n",
        "                if p['y'] < 0 or p['y'] > 1: p['speed_y'] *= -1\n",
        "                Color(0, 1, 0.25, p['opacity'])\n",
        "                size = dp(p['size'])\n",
        "                Ellipse(pos=(p['x'] * self.width - size/2, p['y'] * self.height - size/2), size=(size, size))\n",
        "\n",
        "class GlowingLabel(MDLabel):\n",
